<form method="post" action="{{ path('logins_delete', {'loginid': login.loginid}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ login.loginid) }}">
    <button class="btn">Delete</button>
</form>
